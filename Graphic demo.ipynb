    "\n",
    "t = turtle.Turtle()\n",
    "t.shape(\"turtle\")\n",
    "t.pencolor(\"#ff00ff\") \n",
    "t.pensize(10)\n",
    "\n",
    "t.forward(200)\n",
    "t.right(120)\n",
    "t.forward(200)\n",
    "t.right(120)\n",
    "t.forward(200)\n",
    "\n",
    "\n",
    "turtle.done()"
